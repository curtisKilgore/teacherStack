  "bugs": {
    "url": "https://github.com/cb-ucr-2020/teacher-stack.github.io/issues"
  },
  "homepage": "https://github.com/cb-ucr-2020/teacher-stack.github.io#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "express-validator": "^6.6.0",
    "faker": "^4.1.0",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.27.0",
    "mongoose": "^5.9.21",
    "react-awesome-modal": "^2.0.5",
